#正在製作中~
from __future__ import annotations
class Node:
    def __init__(self):
        self.parent = None
        self.children = None
    
    def addchild(self,n: Node):
        self.children += n
    
class Tree:
    def root(self):
        return self.root
    
    def addchild(self,p: Node,n: Node):
        Node = self.root
        if Node == p:
            Node.addchild
        
        for c in Node.children:
            self.addchild(c,n)

def dump(deck):
    print("tree測試")
    deck.root = d1
    deck.parent = deck.root
    deck.Node.addchild(d2)
    deck.Tree.addchild(d1,d3)

if __name__ == "__main__":
    d1 = Node("A")
    d2 = Node("B")
    d3 = Node("C")
