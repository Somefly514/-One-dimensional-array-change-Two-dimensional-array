from __future__ import annotations
class Tree:
    def __init__(self,data):
        self.array = []
        self.p1 = 0
        self.c1 = 1
        self.parent = self.array[self.p1]
        self.child = self.array
        
    def append(self.data):
        
    
    def root(self):
        return self.parent
    
    def addchild(self,p,n):
        if self.parent == p:
            self.child.append(n)
    
def dump(deck):
    print("用列表做簡單的樹實現")
